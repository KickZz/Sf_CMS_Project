<div class="container-fluid">
    <div class="row">
        <div id="menu" class="col-xs-4 col-md-3 panel-group ">

            <div class="panel panel-default">
                <div class="panel-heading">
                    <h2 class="panel-title">
                        <a class="lienMenu" data-parent="#menu" data-toggle="collapse" href="#page"><i class="fa fa-file-text-o" aria-hidden="true"></i> Page <i id="iconePage" class="fa fa-chevron-circle-right pull-right" aria-hidden="true"></i></a>
                    </h2>
                </div>

                <ul id="page" class="list-group collapse">

                    <li class="list-group-item"><a href="{{ path('add_page') }}" class="ajoutPage">Nouvelle Page</a><span id="addPageSuccess" class="label label-success col-md-offset-1">Page ajoutée</span>{% image 'bundles/SfCmsProjectCms/images/loading.gif' %}<img class="pull-right" id="loadingTemplateAjouterPage" src='{{asset_url }}' alt='loading'>{% endimage %}</li>
                    <li class="list-group-item"><a href="{{ path('view_all_page') }}" class="listPage">Liste Page</a><span id="editPageSuccess" class="label label-success col-md-offset-1">Page éditée</span><span id="supPageSuccess" class="label label-success col-md-offset-1">Page supprimée</span>{% image 'bundles/SfCmsProjectCms/images/loading.gif' %}<img class="pull-right" id="loadingTemplateListPage" src='{{asset_url }}' alt='loading'>{% endimage %}</li>
                </ul>

            </div>
            <div class="panel panel-default">
                <div class="panel-heading">
                    <h2 class="panel-title">
                        <a class="lienMenu" data-parent="#menu" data-toggle="collapse" href="#article"><i class="fa fa-file-text-o" aria-hidden="true"></i> Article <i id="iconeArticle" class="fa fa-chevron-circle-right pull-right" aria-hidden="true"></i></a>
                    </h2>
                </div>

                <ul id="article" class="list-group collapse">

                    <li class="list-group-item"><a href="{{ path('add_post') }}" class="ajoutPost">Nouvel Article</a><span id="addPostSuccess" class="label label-success col-md-offset-1">Article ajouté</span>{% image 'bundles/SfCmsProjectCms/images/loading.gif' %}<img class="pull-right" id="loadingTemplateAjouterPost" src='{{asset_url }}' alt='loading'>{% endimage %}</li>
                    <li class="list-group-item"><a href="{{ path('view_all_post') }}" class="listPost">Liste Article</a><span id="editPostSuccess" class="label label-success col-md-offset-1">Article édité</span><span id="supPostSuccess" class="label label-success col-md-offset-1">Article supprimé</span>{% image 'bundles/SfCmsProjectCms/images/loading.gif' %}<img class="pull-right" id="loadingTemplateListPost" src='{{asset_url }}' alt='loading'>{% endimage %}</li>
                </ul>

            </div>
            <div class="panel panel-default">
                <div class="panel-heading">
                    <h2 class="panel-title">
                        <a class="lienMenu" data-parent="#menu" data-toggle="collapse" href="#personnalisation"><i class="fa fa-pencil-square-o" aria-hidden="true"></i> Personnalisation <i id="iconePersonnalisation" class="fa fa-chevron-circle-right pull-right" aria-hidden="true"></i></a>
                    </h2>
                </div>

                <ul id="personnalisation" class="list-group collapse">

                    <li class="list-group-item"><a href="{{ path('view_template') }}" class="viewTemplatePage">Template Page</a><span id="addTemplatePageSuccess" class="label label-success col-md-offset-1">Template ajouté</span><span id="editTemplatePageSuccess" class="label label-success col-md-offset-1">Template édité</span><span id="removeTemplatePageSuccess" class="label label-success col-md-offset-1">Template supprimé</span>{% image 'bundles/SfCmsProjectCms/images/loading.gif' %}<img class="pull-right" id="loadingViewTemplatePage" src='{{asset_url }}' alt='loading'>{% endimage %}</li>
                    <li class="list-group-item"><a href="{{ path('view_template_post') }}" class="viewTemplatePost">Template Article</a><span id="addTemplatePostSuccess" class="label label-success col-md-offset-1">Template ajouté</span><span id="editTemplatePostSuccess" class="label label-success col-md-offset-1">Template édité</span><span id="removeTemplatePostSuccess" class="label label-success col-md-offset-1">Template supprimé</span>{% image 'bundles/SfCmsProjectCms/images/loading.gif' %}<img class="pull-right" id="loadingViewTemplatePost" src='{{asset_url }}' alt='loading'>{% endimage %}</li>
                    <li class="list-group-item"><a href="{{ path('menu') }}" class="menu">Menu</a><span id="saveMenu" class="label label-success col-md-offset-1">Menu enregistré</span>{% image 'bundles/SfCmsProjectCms/images/loading.gif' %}<img class="pull-right" id="loadingTemplateMenu" src='{{asset_url }}' alt='loading'>{% endimage %}</li>

                </ul>

            </div>
            <div class="panel panel-default">
                <div class="panel-heading">
                    <h2 class="panel-title">
                        <a class="lienMenu" data-parent="#menu" data-toggle="collapse" href="#media"><i class="fa fa-file-image-o" aria-hidden="true"></i> Média <i id="iconeMedia" class="fa fa-chevron-circle-right pull-right" aria-hidden="true"></i></a>
                    </h2>
                </div>

                <ul id="media" class="list-group collapse">

                    <li class="list-group-item"><a href="{{ path('media_storage') }}" class="mediaStorage">Bibliothèque de média</a><span id="removeMediaSuccess" class="label label-success col-md-offset-1">Media supprimé</span>{% image 'bundles/SfCmsProjectCms/images/loading.gif' %}<img class="pull-right" id="loadingMediaStorage" src='{{asset_url }}' alt='loading'>{% endimage %}</li>
                    <li class="list-group-item"><a href="{{ path('add_media') }}" class="addMedia">Ajouter un média</a><span id="addMediaSuccess" class="label label-success col-md-offset-1">Image ajoutée</span>{% image 'bundles/SfCmsProjectCms/images/loading.gif' %}<img class="pull-right" id="loadingAddMedia" src='{{asset_url }}' alt='loading'>{% endimage %}</li>

                </ul>

            </div>
            <div class="panel panel-default">
                <div class="panel-heading">
                    <h2 class="panel-title">
                        <a class="lienMenu" data-parent="#menu" data-toggle="collapse" href="#reglage"><i class="fa fa-cogs" aria-hidden="true"></i> Réglages <i id="iconeReglage" class="fa fa-chevron-circle-right pull-right" aria-hidden="true"></i></a>
                    </h2>
                </div>

                <ul id="reglage" class="list-group collapse">
                    {% if is_granted("ROLE_ADMIN") %}
                        <li class="list-group-item"><a href="{{ path('view_user') }}" class="viewUser">Gestion utilisateur</a><span id="addUserSuccess" class="label label-success col-md-offset-1">Utilisateur ajouté</span><span id="editUserSuccess" class="label label-success col-md-offset-1">Utilisateur édité</span><span id="supUserSuccess" class="label label-success col-md-offset-1">Utilisateur supprimé</span>{% image 'bundles/SfCmsProjectCms/images/loading.gif' %}<img class="pull-right" id="loadingViewUser" src='{{asset_url }}' alt='loading'>{% endimage %}</li>
                    {% endif %}
                    <li class="list-group-item"><a href="{{ path('edit_profil_user') }}" class="viewProfilUser">Profil</a><span id="editProfilUserSuccess" class="label label-success col-md-offset-1">Profil édité</span>{% image 'bundles/SfCmsProjectCms/images/loading.gif' %}<img class="pull-right" id="loadingViewProfilUser" src='{{asset_url }}' alt='loading'>{% endimage %}</li>

                </ul>

            </div>

        </div>
            <div id="templateLoad" class="col-xs-offset-4 col-md-offset-3 col-md-9"></div>
    </div>
</div>
